<?xml version="1.0" encoding="UTF-8"?>
<jboss-deployment-structure>
	<deployment>
		<dependencies>
			<module name="org.codehaus.jackson.jackson-core-asl" export="true" />
			<module name="org.codehaus.jackson.jackson-jaxrs" export="true" />
			<module name="org.codehaus.jackson.jackson-mapper-asl" export="true" />
		</dependencies>
		<exclusions>
			<module name="org.apache.catalina" />
			<module name="org.apache.coyote" />
			<module name="org.apache.juli.logging" />
			<module name="org.apache.tomcat" />
		</exclusions>
	</deployment>
</jboss-deployment-structure>